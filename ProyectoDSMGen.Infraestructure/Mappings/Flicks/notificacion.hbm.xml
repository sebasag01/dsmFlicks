<?xml version="1.0" encoding="utf-8" ?>
		<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		                   assembly="ProyectoDSMGen.Infraestructure"
		                   namespace="ProyectoDSMGen.Infraestructure.EN.Flicks">
		    
		    
			
				<class name="NotificacionNH" table="Notificacion"> 
				  	
						<id name="Id" column="idNotificacion" type="integer"> 
							<generator class="hilo"/>
						</id>
			
					
					<property name="Mensaje" type="string"> 
						<column name="mensaje"  not-null="true"/>
					</property>
					<property name="Fecha" type="date"> 
						<column name="fecha"  not-null="true"/>
					</property>
					<property name="TipoNotificacion" > 
						<column name="tipoNotificacion"  not-null="true"/>
					</property>
					<property name="IdOrigen" type="long"> 
						<column name="IdOrigen"  not-null="true"/>
					</property>
					<property name="EstadoNoti" > 
						<column name="estadoNoti"  not-null="true"/>
					</property>
			
			
			
		  			
						<many-to-one name="Usuario" class="ProyectoDSMGen.Infraestructure.EN.Flicks.UsuarioNH" >	
								<column name="FK_id_idUsuario"/> 
						</many-to-one>
											 					
			
			
			
			
			
					
					
					
					
					
					
					
		
		 </class>	
		  
		  <query name="NotificacionNHReadAllHQL">from NotificacionNH</query>
		  <query name="NotificacionNHreadFilterEstadoHQL"><![CDATA[FROM NotificacionNH n WHERE n.EstadoNoti = :estado]]></query>
		  <query name="NotificacionNHreadFIlterOrigenHQL"><![CDATA[FROM NotificacionNH n WHERE n.IdOrigen = :idOrigen]]></query>
		</hibernate-mapping>
	