<?xml version="1.0" encoding="utf-8" ?>
		<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		                   assembly="ProyectoDSMGen.Infraestructure"
		                   namespace="ProyectoDSMGen.Infraestructure.EN.Flicks">
		    
		    
			
				<class name="AdministradorNH" table="Administrador"> 
				  	
						<id name="Id" column="id" type="integer"> 
							<generator class="hilo"/>
						</id>
			
					
					<property name="Nombre" type="string"> 
						<column name="nombre"  not-null="true"/>
					</property>
					<property name="Email" type="string"> 
						<column name="email"  not-null="true"/>
					</property>
					<property name="Pass" type="string"> 
						<column name="pass"  not-null="true"/>
					</property>
			
			
			
			
			
		  			
						<bag name="Pelicula" >
							<key>
							
								
								<column name="FK_id_idAdministrador"/>
					      	</key>
							<one-to-many class="ProyectoDSMGen.Infraestructure.EN.Flicks.PeliculaNH"/>
					    </bag>
					
			
		  			
						<bag name="Reporte" >
							<key>
							
								
								<column name="FK_id_idAdministrador"/>
					      	</key>
							<one-to-many class="ProyectoDSMGen.Infraestructure.EN.Flicks.ReporteNH"/>
					    </bag>
					
			
					
					
					
					
					
					
		
		 </class>	
		  
		  <query name="AdministradorNHReadAllHQL">from AdministradorNH</query>
		</hibernate-mapping>
	